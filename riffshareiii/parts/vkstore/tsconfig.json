{
	"compilerOptions": {
		"target": "es2015",
		"outFile": "testapp.js",
		"strictNullChecks": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"noResolve": false,
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": false,
		"experimentalDecorators": true,
		"isolatedModules": false,
		"module": "AMD",
		"lib": [
			"dom",
			"es2016",
			"es5"
		],
		"declaration": true,
		"types": []
	},
	"files": [
        "app.ts"
	]
}
